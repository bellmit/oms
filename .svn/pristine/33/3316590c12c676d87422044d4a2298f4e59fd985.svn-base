.regBtn{
    margin-top: 20px;
}
.regBtn button{
    width: 200px;
}
.regSucess{
    color: #ff6000;
    text-align: center;
    margin-bottom: 30px;
}
.reg-sbtn{
    margin: 26px auto;
}
.registerTip{
    color: #1083B1;
    float: right;
    margin-top: 15px;
}
.readFlag{
    margin-top: 2px;
}
.alert {
    width: 30%;
    position: fixed;
    left: 50%;
    top: 45%;
    z-index: 1001;
    text-align: center;
    border: none;
    color: #ffffff!important;
    margin: 0 0 0 -15%!important;
    padding: 10px!important;
}
.alert-danger h4{
    color: #ffffff!important;
}
.alert-dismissible .close{
    right: 0;
}
.alert>p{
    margin-top: 10px;
}


