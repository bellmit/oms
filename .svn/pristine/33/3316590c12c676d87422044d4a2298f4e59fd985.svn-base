.typeInfo .text-info{
    width: 55%;
    float: left;
    margin-left: 15px;
    color: #544b4b;
    text-align: left;
    margin-top: 30px;
}
.typeInfo .text-info span{
    width: 100%;
    font-size: 12px;
    float: left;
}
.typeInfo .text-info span.typeTitl{
    font-size: 18px;
}
.typeInfo:hover{
    transform: scale(0.9);
    -webkit-transform: scale(0.9);
    -moz-transform: scale(0.9);
}
.indexFooter{
    height: 80px;
    line-height: 80px;
    background: #dfdee3;
    width: 100%;
    text-align: center;
    font-size: 12px;
    color: #544b4b;
}
.typeNews{
    position: absolute;
    left: 29%;
    top: 0;
}
.typeNews .badge{
    font-size: 14px;
    background: #f00;
    font-weight: normal;
}
/*注册*/
.topBan{
    width: 100%;
    height: 120px;
    overflow: hidden;
    background: #b5e1f3 url("../images/register_banner.png") center top no-repeat;
}
.content-section{
    width: 1100px;
    margin: 0 auto;
}
.topBan .logo{
    display: block;
    float: left;
    width: 420px;
    max-width: 420px;
    height: 120px;
    overflow: hidden;
}
.topBan .logo img{
    margin-top: 20px;
}
.topBan .returnHome{
    float: right;
    margin-top: 20px;
}
.regTitl{
    width: 90%;
    margin: 0 auto 20px;
    border-bottom: 1px dashed #dddddd;
    padding: 5px 0;
    height: 40px;
}
.regTitl p,.regTitl span{
    float: left;
    width: 50%;
}
.regTitl span{
    font-size: 16px;
}
.regTitl p{
    float: right;
    text-align: right;
}
.contentNav{
    background: #f9f9f9;
    padding: 60px 0 100px;
}
.reg-wrap{
    background: #fff;
    border: 1px solid #dddddd;
    border-radius: 3px;
    -webkit-border-radius: 3px;
    padding: 20px 0;
}
input.error,textarea.error,select.error{
    border-color: #f00;
}
.error{
    color: #f00!important;
}
input.right,textarea.right,select.right{
    border-color: #3dcca2;
}
