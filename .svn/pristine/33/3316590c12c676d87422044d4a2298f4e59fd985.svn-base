a,button{
    blr:hide-focus:expression_r(this.hideFocus=true);
    outline-style:none;
    transition: all .2s ease;
    -webkit-transition: all .2s ease;
    -moz-transition: all .2s ease;
}
a:hover,a:focus{
    text-decoration: none;
}
input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {
    background-color: rgb(255, 255, 255)!important;
    background-image: none;
    color: rgb(0, 0, 0);
}
.fn-clear:after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
    zoom: 1;
}
.loginBody{
    background: #eaeaf1;
}
.loginNav .input-group input::-webkit-input-placeholder {
    color: #aaaaaa;
}
.loginNav .input-group input:-moz-placeholder {
    color: #aaaaaa;
}
.loginNav .input-group input.form-control{
    height: 32px!important;
}
.loginBox{
    width: 100%;
    height: 600px;
    background: #97d0ea;
    position: relative;
 }
.registerBox{
    height: 700px;
}
.loginBox .loginForm{
    width: 364px;
    height: 302px;
    position: absolute;
    left: 50%;
    top: 50%;
    margin: -151px 0 0 -182px;
}
.registerBox .loginForm{
    margin: -300px 0 0 -182px;
}
.loginBox .logoImg{
    margin-left: 8px;
}
.loginBox img,.bannerNav img{
    display: block;
}
.loginNav{
    background: #ffffff;
    border-radius: 4px;
    padding: 10px 0;
    margin: 26px 0 15px 10px;
    width: 100%;
}
.loginNav .input-group{
    padding: 10px 40px 10px 27px;
    margin: 5px 0;
}
.loginNav .input-group:first-child{
    border-bottom: 1px solid #efeff4;
}
.loginNav .input-group input,.loginNav .input-group input:focus,.loginNav .input-group input:active{
    border: none;
    box-shadow: none;
}
.loginNav .input-group-addon{
    background: none;
    border: none;
}
.loginNav .input-group-addon i{
    color: #d4d4d4;
    font-size: 24px;
}
.loginBox .btn-primary{
    width: 100%;
    margin-left: 10px;
    background-color: #042a3c;
}
.footer{
    height: 40px;
    width: 100%;
    background: #efeff4;
    font-size: 12px;
    text-align: center;
    line-height: 40px;
    color: #544b4b;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 1;
}
.loginfooter{
    width: 100%;
    line-height: 40px;
    color: #544b4b;
    text-align: center;
    font-size: 12px;
}
/*=============== Index css =========================*/
.navbar-nav .loginTime{
    color: #CACCCE;
    line-height: 50px;
}
.indexbody{
    background: #efeff4;
    overflow-x: hidden;
}
.navbar{
    background: #042a3c;
    color: #efeff4;
    border-radius: 0;
    margin-bottom: 0;
    font-size: 14px;
    position: fixed;
    z-index: 2;
    width: 100%;
    top: 0;
    left: 0;
}
.navbar-nav{
    float: right;
}
.navbar-nav i.fa{
    font-size: 18px;
    color: #ffffff;
}
.navbar-nav .badge{
    background: #ff0000;
    position: absolute;
    top: 12px;
    left: 22px;
    padding: 1px 5px;
    font-size: 7px;
    font-weight: normal;
}
.user-menu a{
    color: #efeff4;
    font-size: 14px;
}
.user-menu a:hover{
    color: #337ab7;
}
.navbar .navbar-header-right a{
    margin: 0 5px;
    cursor: pointer;
    color: #53c9ff;
    font-size: 12px;
}
.navbar .navbar-header-right p{
    font-size: 12px;
    color: #999999;
}
.navbar .navbar-header-right a:hover{
    color: #337ab7;
}
.nav>li>a:focus, .nav>li>a:hover{
    background: none;
    color: #dadada;
}
.Indexline-bg{
    top: 50px;
}
.bannerBox{
    width: 100%;
    background: #b9e0f2;
    height: 179px;
    margin-top: 50px;
}
.bannerBox img{
    display: block;
    margin: 0 auto;
    width: 1200px;
    max-height: 179px;
    overflow: hidden;
}
.dexContent{
    width: 78%;
