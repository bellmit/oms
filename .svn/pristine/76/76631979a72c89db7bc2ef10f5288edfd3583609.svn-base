/*弹窗*/
.am-dialog:not([am-version]) {
    position: absolute;
    -webkit-box-sizing: border-box;
    padding: 0 0 10px 0;
}
.am-dialog:not([am-version]) .am-dialog-wrap {
    padding-top: 16px;
    /*max-width: 420px;*/
    max-width: 100%; 
    margin: 0 auto;
    border-radius: 7px;
    -webkit-background-clip: padding-box;
    background-color: #F8F8F8;
    overflow: hidden;
}
.am-dialog:not([am-version]) .am-dialog-header {
    -webkit-box-sizing: border-box;
}
.am-dialog:not([am-version]) .am-dialog-header h3 {
    font-size: 17px;
    line-height: 50px;
    text-align: center;
    color: #000;
    font-weight: 200;
    font-family: "microsoft yahei";
}
.am-dialog:not([am-version]) .am-dialog-body .am-dialog-brief {
    display: block;
    font-size: 13px;
    line-height: 21px;
    text-align: center;
    color: #000;
}
