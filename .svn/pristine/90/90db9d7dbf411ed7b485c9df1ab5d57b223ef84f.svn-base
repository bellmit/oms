<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<!--品牌增加-->
<!--------------- Content start ----------------->
<div>
	<div class="content-wrapper">
	<div class="col-md-11 pageTitl">
		<h2 class="am-ft-16 fn-left">添加品牌</h2>
		<a href="javascript:;" class="line-btn fn-right" ng-click="goback()">返回</a>
	</div>
	<div class="fn-clear"></div>

	<section class="container-fluid">
		<div class="row">
			<div class="prodet-nav">
				<div class="prodet-box">
					<!-- row1 -->
					<div class="row">
						<div class="col-md-1 inprodet-titl am-ft-14 mgt25">
							<em class="am-ft-red mgr5">*</em>品牌logo：
						</div>
						<div class="col-md-3 form-group memImg needValInfo">
							<input type="file" ngf-select="uploadFilesLogo($files)"/>
							<img id="logoimg" src="../static/base/images/icon_upload.png" />
						</div>
						<p class="am-ft-red note-brandAdd">(请上传700*700以上图片，小于3M)</p>
					</div>
					<!-- row2 -->
					<div class="row">
						<div class="col-md-1 inprodet-titl am-ft-14"><em class="am-ft-red mgr5">*</em>品牌名称：</div>
						<div class="col-md-3 form-group needValInfo">
							<input type="text" class="col-md-8 text-overflow" id="brandNamea"/>
						</div>
					</div>
				</div>
			</div>
		</div>
		<button type="button" class="btn btn-primary addBrandBtn" ng-click="addBrand()">确认添加</button>
		<button type="button" class="btn btn-default mgl10" ng-click="goback()">取消</button>
	</section>
</div>
</div>