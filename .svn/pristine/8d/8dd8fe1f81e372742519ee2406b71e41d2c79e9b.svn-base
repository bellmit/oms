    var clipboard = new Clipboard('.copyurl');
    clipboard.on('success', function(e) {
        console.log(e);
        alertmsg("复制成功"+e.text)
    });
    clipboard.on('error', function(e) {
        console.log(e);
    });
    $scope.$on("$stateChangeStart",function(){
		clipboard.destroy();
    })
	
