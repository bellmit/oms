<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<table class="table contractTable areaTable">
	<tr>
		<th>地区</th>
		<th class="fn-clear">
			<div class="thBox fn-clear">
				<span class="fn-left">销售总金额</span>
				<div class="arrowPart fn-left mgl5">
					<i class="fa fa-angle-up arrowActive"></i>
					<i class="fa fa-angle-down"></i>
				</div>
			</div>
		</th>
		<th>
			<div class="thBox fn-clear">
				<span class="fn-left">新增客户</span>
				<div class="arrowPart fn-left mgl5">
					<i class="fa fa-angle-up arrowActive"></i>
					<i class="fa fa-angle-down"></i>
				</div>
			</div>
		</th>
		<th>
			<div class="thBox fn-clear">
				<span class="fn-left">合同数</span>
				<div class="arrowPart fn-left mgl5">
					<i class="fa fa-angle-up arrowActive"></i>
					<i class="fa fa-angle-down"></i>
				</div>
			</div>
		</th>
		<th>
			<div class="thBox fn-clear">
				<span class="fn-left">未到期合同数</span>
				<div class="arrowPart fn-left mgl5">
					<i class="fa fa-angle-up arrowActive"></i>
					<i class="fa fa-angle-down"></i>
				</div>
			</div>
		</th>
		<th>
			<div class="thBox fn-clear">
				<span class="fn-left">待收金额</span>
				<div class="arrowPart fn-left mgl5">
					<i class="fa fa-angle-up arrowActive"></i>
					<i class="fa fa-angle-down"></i>
				</div>
			</div>
		</th>
	</tr>
	<tr class="inServer" id="{{groupList.province}}" ng-repeat="groupList in groupLists">
		<td class="col-lg-2"><a href="javascript:;" ng-click="lookCityList(this.groupList.province)">{{groupList.province}}<i class="mgl5 fa fa-angle-down"></i></a></td>
		<td class="deepColor col-lg-2">￥{{groupList.allTotalprice}}</td>
		<td class="col-lg-2">{{groupList.addShopCount}}</td>
		<td class="col-lg-2">{{groupList.contractCount}}</td>
		<td class="col-lg-2">{{groupList.notDueCount}}</td>
		<td class="deepColor col-lg-2">￥{{groupList.notPaidAmount}}</td>
	</tr>
