<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<!--------------- Content start ----------------->
    <div class="content-wrapper content-wrapper-commerciaInfo mgb15">
        <div class="col-md-11 pageTitl">
            <h2 class="am-ft-16 fn-left">帐户资料</h2>
            <!--<a href="javascript:;" class="btn btn-primary fn-right">编辑</a>-->
        </div>
        <div class="fn-clear"></div>

        <div class="ManColTable col-lg-12">
            <div class="commercia-info">
                <!-- row1 -->
                <div class="row">
                    <div class="nowraps-text inprodet-titl am-ft-14" style="line-height: 28px;">帐户：</div>
                    <p>{{userName}}</p>
                </div>
                <!-- row2 -->
                <div class="row">
                    <div class="nowraps-text inprodet-titl am-ft-14"  style="line-height: 29px;">昵称：</div>
                    <div class="col-md-3 form-group needValInfo" style="margin-bottom: 5px;">
                        <input type="text" class="col-md-8 text-overflow" name=""  ng-model="trueName" ng-value="trueName">
                    </div>
                </div>
                <!-- row3 -->
                <div class="row">
                    <div class="nowraps-text inprodet-titl am-ft-14"  style="line-height: 36px;">邮箱：</div>
                    <div class="col-md-3 form-group needValInfo" style="margin-bottom: 15px;">
                        <input type="text" class="col-md-8 text-overflow" name=""  ng-model="email" ng-value="email">
                    </div>
                </div>
                <div class="fn-clear"></div>
            </div>
            <button type="button" class="btn btn-primary addBrandBtn" ng-click="sureEdit()">确认修改</button>
            <button type="button" class="btn btn-default mgl10" ng-click="goback()">取消</button>
            <div class="fn-clear"></div>
        </div>
        <div class="fn-clear"></div>
    </div>

</div>
